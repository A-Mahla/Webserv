
server	{

	server_name					YO.com;

	listen						2020;

	autoindex					on;

	error_page					500 502 503 504	/ 50x.html ;

	index						index.html index.php test.html ;

	root						.;

	client_body_buffer_size		8k;


	methodes							POST:GET:DELETE;

	autoindex							on;

	location		./srcs/class/ {
				methodes					POST:GET:DELETE;
				autoindex					off;
	}
}

server	{

	server_name					example.com;

	listen						127.0.0.1:8080;

	autoindex					on;

	error_page					500 502 503 504	/ 50x.html ;
	error_page					404				/ Makefile;

	index						index.html index.php test.html ;

	root						./;

	client_body_buffer_size		8k;

	methodes        			POST:GET:DELETE;

	location		./config {

				autoindex		off;
				root			.;
	}

	location		./Client.cpp {
				root srcs/class;
	}

	location	./test_delete/ {
				root 		./;
				methodes	DELETE;
	}
}





